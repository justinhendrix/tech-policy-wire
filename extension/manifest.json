{
  "manifest_version": 3,
  "name": "Tech Policy Press Field Notes Clipper",
  "version": "1.0.0",
  "description": "Submit links to Tech Policy Press Field Notes for review",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
